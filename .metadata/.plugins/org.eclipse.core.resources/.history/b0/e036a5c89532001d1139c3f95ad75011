package com.simplilearn.hibernate.client;

import com.simplilearn.hibernate.dto.Customer;
import com.simplilearn.hibernate.service.CustomerService;
import com.simplilearn.hibernate.service.CustomerServiceImpl;

public class Client {
	 public static void main(String args[])
	 {
		CustomerService service =new CustomerServiceImpl();
		Customer customer=new Customer("Ansul","Bangalore");
		int custId=service.addCustomer(customer);
		System.out.println("CustID : "+custId);
	 }
}
