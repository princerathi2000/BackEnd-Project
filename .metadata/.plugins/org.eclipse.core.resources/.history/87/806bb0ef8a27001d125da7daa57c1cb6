	public static void main(String args[])
	{
		try
		{
			Class.forName("com.mysql.jdbc.Driver");
			Connection con=DriverManager.getConnection("jdbc:mysql://localhost:3306/mytrgdb","root","");
	
		}
		catch(Exception ex)
		{
			
		}
	}
}
